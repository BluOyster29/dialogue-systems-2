<?xml version="1.0" encoding="utf-8"?>
<domain name="CallJohnDomain" is_super_domain="true">
  <goal type="perform" action="top">
    <plan>
      <forget_all/>
      <findout type="goal"/>
    </plan>
  </goal>

  <goal type="perform" action="make_call">
    <plan>
      <findout type="wh_question" predicate="contact_name"/>
      <findout type="wh_question" predicate="number_type"/>
      <invoke_service_action name="MakeCall" postconfirm="True"/>
    </plan>
  </goal>

  <goal type="perform" action="contact_lookup">
    <plan>
      <findout type="wh_question" predicate="contact_name"/>
      <findout type="wh_question" predicate="number_type"/>
      <invoke_service_action name="ContactLookup" postconfirm="True"/>
    </plan>
  </goal>
</domain>
